# Пример файла переменных окружения
# Скопируйте в .env и настройте под ваши нужды

# GitHub токен для увеличения лимитов API (опционально)
GITHUB_TOKEN=your_github_token_here

# Базовый URL сайта (для GitHub Pages)
SITE_BASE_URL=https://username.github.io/repository-name

# Режим отладки
DEBUG=false

# Максимальное время кеша в часах
CACHE_MAX_AGE=12

# Максимальное количество одновременных загрузок
MAX_CONCURRENT_DOWNLOADS=5